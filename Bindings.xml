<Bindings>
	<Binding name='Raid Marker' header='ELRONSUI' runOnUp='true'>
		RaidMarker_HotkeyPressed(keystate)
	</Binding>
	<Binding name='Farm Mode' runOnUp='false'>
		FarmMode()
	</Binding>
	<Binding name='Toggle Chat (Left)' runOnUp='false'>
		ToggleChat('left')
	</Binding>
	<Binding name='Toggle Chat (Right)' runOnUp='false'>
		ToggleChat('right')
	</Binding>
	<Binding name='Toggle Chat (Both)' runOnUp='false'>
		ToggleChat('both')
	</Binding>
	<Binding name='Scroll full up' runOnUp='true'>
		ChatScrollUp_HotkeyPressed(keystate)
	</Binding>
	<Binding name='Scroll full down' runOnUp='true'>
		ChatScrollDown_HotkeyPressed(keystate)
	</Binding>
</Bindings>